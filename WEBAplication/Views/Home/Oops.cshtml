
@{
    ViewBag.Title = "Oops";
    Layout = "~/Views/Shared/_LayoutError.cshtml";
}

<div class="col">
    <div class="row divback">
        <div class="col">
            <div class="row text-white text-center">
                <h2>WEB APLICATION</h2>
            </div>
        </div>
    </div>

    <div class="row mt-5 divback">
        <div class="col">
            <div class="row text-white text-center">
                <h2>Erro ao Carregar a Pagina @ViewBag.Error</h2>
            </div>
            <div class="row text-white text-center">
                <h2>Entre em contato com o Administrador do Sistema!</h2>
            </div>
        </div>
    </div>

    <div class="row mt-5 divback">
        <div class="col">
            <div class="row text-white text-center">
                <h2>Pedimos desculpas pelo Ocorrido!</h2>
            </div>
        </div>
    </div>

    <div class="row mt-5">
        <div class="col">
            <div class="row text-white justify-content-center">
               <button class="btn btnHome btn-info w-50" onclick="GoLogin()">VOLTAR PARA LOGIN</button>
            </div>
        </div>
    </div>

</div>



